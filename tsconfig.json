{
    "compilerOptions": {
        "types": [
            "hammerjs",
            "node"
        ],
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": false,
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "outDir": "out",
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "out",
        "node_modules",
        "dist",
        "dev",
        "reports"
    ],
    "version": "1.8.10"
}